{
  "env": {
    "es2021": true,
    "node": true,
    "browser": true
  },
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "rules": {
    "no-unused-vars": "off",
    "no-use-before-define": "off",
    "no-console": "off",
    "consistent-return": "off",
    "no-underscore-dangle":"off"
  }
}
